{
  "name": "JuniorIT.AI GameCraft",
  "build": {
    "dockerfile": "Dockerfile"
  },
  "hostRequirements": {
    "cpus": 2
  },
  "overrideCommand": false,
  "workspaceMount": "source=${localWorkspaceFolder},target=/home/juniorit/workspace/${localWorkspaceFolderBasename},type=bind,consistency=cached",
  "workspaceFolder": "/home/juniorit/workspace",
  "mounts": [
    "source=/home/frankhe/Dropbox/github_workspace/py-game-project,target=/home/juniorit/workspace/py-session-1,type=bind,consistency=cached",
    "source=/home/frankhe/Dropbox/github_workspace/ts-game-project,target=/home/juniorit/workspace/ts-session-1,type=bind,consistency=cached",
    "source=/home/frankhe/Dropbox/github_workspace/dart-game-project,target=/home/juniorit/workspace/dart-session-1,type=bind,consistency=cached"
  ],
  "containerEnv": {
    "GAMECRAFT_PROJECT_PATH": "/home/juniorit/workspace/${localWorkspaceFolderBasename}"
  },
  "waitFor": "onCreateCommand",
  "customizations": {
    "vscode": {
      "extensions": [
        "JuniorIT-AI.vscode-juniorit",
        "ms-vscode.cpptools-extension-pack",
        "bmewburn.vscode-intelephense-client",
        "kevinrose.vsc-python-indent",
        "ms-python.python",
        "ms-python.autopep8",
        "ms-vscode.vscode-typescript-next"
      ],
      "settings": {
        "extensions.ignoreRecommendations": true
      }
    }
  },
  "portsAttributes": {
    "9000": {
      "label": "Application",
      "onAutoForward": "openPreview"
    }
  },
  "forwardPorts": [
    9000
  ]
}